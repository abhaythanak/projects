{
    "manifest_version": 3,
    "name": "pomodoro Timer",
    "version": "0.1.0",
    "description": "Helps you focus and get things done!",
    "icons": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_title": "Timer",
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "notifications"]
    }
  